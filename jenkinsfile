pipeline {
    agent : any
    
    stages {
         stage('Checkout') {
             steps {
                checkout ([
                $class: 'GitSCM',
                Branches: [[name: '*/main']],
                userRemoteConfigs: [[ url: '<put git ulr>',
                credentials: 'github'
                ]]])
            }
         }
         stage('Build') {
             steps {
                sh'''
                sudo apt update 
                sudo apt install -y maven
                ls-lrt
                cd <// file path>
                maven clean install
                '''
            }
         }
    } 
}
